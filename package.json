{
  "name": "etch-a-skeetch",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "compile": "sass --watch --no-source-map scss/style.scss css/style.css",
    "build:compile": "sass --no-source-map scss/style.scss build/compiled.css",
    "build:prefixes": "postcss build/compiled.css --use autoprefixer --output build/prefixed.css",
    "build:optimize": "csso build/prefixed.css --output build/style.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JcSilverX/Etch-A-Sketch.git"
