    "url": "https://github.com/JcSilverX/Etch-A-Sketch/issues"
  },
  "homepage": "https://github.com/JcSilverX/Etch-A-Sketch#readme",
  "dependencies": {
    "autoprefixer": "^10.4.16",
    "csso-cli": "^4.0.2",
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0",
    "sass": "^1.69.5"
  }
}
